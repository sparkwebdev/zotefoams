{
  "name": "underscores",
  "version": "1.0.0",
  "type": "module",
  "description": "Hi. I'm a starter theme called _s, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/_s#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/_s.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/_s/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@playwright/test": "^1.51.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^22.13.13",
    "autoprefixer": "^10.4.21",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.1.2",
    "cssnano": "^7.1.0",
    "glob": "^11.0.3",
    "open": "^10.1.2",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.46.2",
    "rollup-plugin-scss": "^4.0.1",
    "sass": "^1.89.0",
    "stylelint-config-prettier": "^9.0.5"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "build": "rollup -c",
    "lint": "stylelint 'src/sass/**/*.scss' --fix && prettier --write 'src/sass/**/*.scss'",
    "start": "concurrently \"rollup -c --watch\" \"browser-sync start --proxy 'https://zotefoams-phase-2.local/' --https --files './style.css, js/bundle.js, ./**/*.php'\"",
    "test": "rimraf tests/vrc/results && VRC_DEVICES=desktop-only playwright test tests/vrc/compare-pages.spec.js",
    "test:all": "rimraf tests/vrc/results && VRC_LEVEL=3 VRC_MAX_FAILURES=20 VRC_DEVICES=desktop-only playwright test tests/vrc/compare-pages.spec.js"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "pixelmatch": "^7.1.0",
    "pngjs": "^7.0.0",
    "sharp": "^0.34.1",
    "xml2js": "^0.6.2"
  }
}
